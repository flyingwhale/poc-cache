{
    "name": "flyingwhale/poc-cache",
    "license": "Apache-2.0",
    "type": "library",
    "keywords": ["cache", "caching", "poc"],
    "authors": [
        {
          "name":   "Imre Toth",
          "email":  "tothimre@gmail.com"
        }
    ],
   "require": {
        "php":                      ">=5.3.0",
        "symfony/class-loader":     "2.*",
        "predis/predis":            "0.7.3",
	"flyingwhale/optionable":    "dev-master"
    },
   "require-dev": {
        "phpunit/phpunit":           "3.7.*",
        "fabpot/php-cs-fixer":      "dev-master",
        "mockery/mockery":          "dev-master"
    },
   "autoload": {
        "psr-0": { "Poc": "src/" }
    },

    "minimum-stability": "dev",
    "process-timeout": "3600"
}
